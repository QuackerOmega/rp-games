-- quacker obenga!!1!!@!121
local count = 0
local cd = game:GetService("Workspace").JobBox.ClickDetector

while wait(.1) do 
if not getgenv().crash then break end
    fireclickdetector(cd)
    for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
        if v.Name == "Durian" then v.Parent = v.Parent.Parent.Character end            
    end
end

while wait(2) do
    for i,v in next, game.Players.LocalPlayer.Character:GetChildren() do
        if v:IsA("Tool") then count = count+1 end
    end
    warn("items "..count)
        if count >= 300 then getgenv().crash = false 
            for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
                v.Parent = v.Parent.Parent.Character
            end

            game.Players.LocalPlayer.Character.Humanoid:Destroy()
        end
    count = 0
end
